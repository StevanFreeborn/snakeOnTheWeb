{
  "name": "snakeontheweb",
  "version": "1.0.0",
  "description": "A version of snake you can play in the browser.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "NODE_ENV=test c8 mocha --timeout 30000 ./tests/**/*.spec.js --exit",
    "dev-windows": "set NODE_ENV=development&& nodemon server.js",
    "test-windows": "set NODE_ENV=test&& c8 mocha --timeout 30000 ./tests/**/*.spec.js --exit"
  },
  "keywords": [
    "snake"
  ],
  "author": "stevan freeborn",
  "license": "MIT",
  "dependencies": {
    "c8": "^7.12.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mocha": "^10.1.0",
    "morgan": "^1.10.0",
    "nanoid": "^4.0.0",
    "nodemon": "^2.0.20",
    "socket.io": "^4.5.3",
    "socket.io-client": "^4.5.3",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "type": "module"
}
